{
  "name": "siemens-element",
  "version": "49.0.1",
  "description": "Siemens Element open source theme",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/spike-rabbit/element.git"
  },
  "author": {
    "name": "Siemens AG",
    "email": "info@siemens.com"
  },
  "type": "module",
  "engines": {
    "node": "^20.0.0 || ^22.0.0",
    "yarn": "Please use NPM to install dependencies",
    "npm": "10 - 11"
  },
  "commitlint": {
    "extends": [
      "@siemens/commitlint-config/.commitlintrc.js"
    ]
  },
  "prettier": "@siemens/prettier-config/.prettierrc.json",
  "scripts": {
    "start": "ng serve",
    "format": "prettier --log-level warn --write .",
    "lint:format": "prettier --check .",
    "lint:commit": "commitlint --from=origin/main",
    "lint:scss": "stylelint **.scss",
    "lint:ng": "ng lint",
    "lint:playwright:eslint": "eslint playwright",
    "build:examples": "ng build",
    "build:examples:prod": "ng build --configuration production",
    "build:examples:aot": "ng build --configuration aot",
    "build:all": "npm run translate:build && npm run lib:build && npm run live-preview:build && npm run charts:build && npm run native-charts:build && npm run dashboards:build",
    "build:all:update-translatable-keys": "npm exec update-translatable-keys",
    "lib:build": "ng build element-ng && cpy \"projects/element-ng/*.scss\" \"./dist/@spike-rabbit/element-ng\" && cpy \"{LICENSE.md,README.md}\" \"./dist/@spike-rabbit/element-ng/\"",
    "lib:test": "ng test element-ng",
    "lib:test:browser": "cross-env HEADLESS=0 ng test element-ng",
    "translate:test": "ng test element-translate-ng",
    "translate:build": "ng build element-translate-ng && cpy LICENSE.md \"./dist/@spike-rabbit/element-translate-ng/\"",
    "docs:build": "uv run mkdocs build --strict",
    "docs:build:api": "cross-env DOCS_COMPOSER=true DOCS_COMPOSER_OPTIONS='{ \"outputDir\": \"docs-out\", \"structured\": true, \"structuredRemoveEncoded\": true }' uv run --active mkdocs build --strict",
    "docs:build:generate": "cross-env DOCS_COMPOSER=true DOCS_COMPOSER_GENERATE=true DOCS_COMPOSER_OPTIONS='{ \"outputDir\": \"docs-out\", \"structured\": true, \"structuredRemoveEncoded\": true, \"structuredExcludeGenerated\": true }' uv run --active mkdocs build --strict",
    "docs:serve": "uv run mkdocs serve",
    "docs:serve:api": "cross-env DOCS_COMPOSER=true uv run --active mkdocs serve",
    "docs:serve:generate": "cross-env DOCS_COMPOSER=true DOCS_COMPOSER_GENERATE=true DOCS_COMPOSER_OPTIONS='{ \"outputDir\": \"docs-out\" }' uv run --active mkdocs serve",
    "live-preview:build": "ng build live-preview && cpy LICENSE.md \"./dist/@spike-rabbit/live-preview/\"",
    "prepare": "husky; exit 0",
    "prepare-brand": "cpy --flat node_modules/@simpl/brand/src/favicon/sie-favicon_internet_64px.png docs/_src --rename=favicon.png && cpy --flat node_modules/@simpl/brand/src/favicon/sie-favicon_internet_64px.png src --rename=favicon.png",
    "postversion": "npm run format",
    "charts:test": "ng test charts-ng",
    "charts:build": "ng build charts-ng && cpy \"{LICENSE.md,projects/charts-ng/README.md}\" \"./dist/@spike-rabbit/charts-ng/\"",
    "native-charts:test": "ng test native-charts-ng",
    "native-charts:build": "ng build native-charts-ng && cpy LICENSE.md \"./dist/@spike-rabbit/native-charts-ng/\"",
    "dashboards:test": "ng test dashboards-ng",
    "dashboards:build": "ng build dashboards-ng && cpy LICENSE.md \"./dist/@spike-rabbit/dashboards-ng/\"",
    "dashboards-demo:start": "ng serve dashboards-demo",
    "dashboards-demo:start:prod": "http-server dist/dashboards-demo -s -p 4201 -a 127.0.0.1",
    "dashboards-demo:start:webcomponents": "http-server dist/dashboards-demo-webcomponents -s -p 4202",
    "dashboards-demo:start:mfe": "ng serve dashboards-demo-mfe",
    "dashboards-demo:build": "ng build dashboards-demo",
    "dashboards-demo:build:demo": "ng build dashboards-demo --configuration demo-deployment",
    "dashboards-demo:build:prod": "ng build dashboards-demo --configuration production",
    "dashboards-demo:build:webcomponents": "ng build dashboards-demo-webcomponents --configuration production --output-hashing none && node projects/dashboards-demo/webcomponents/concat.cjs",
    "dashboards-demo:build:mfe": "ng build dashboards-demo-mfe --configuration production --output-hashing none"
  },
  "workspaces": [
    "projects/element-ng",
    "projects/element-translate-cli",
    "projects/element-translate-ng",
    "projects/element-theme",
    "projects/live-preview",
    "projects/charts-ng",
    "projects/native-charts-ng",
    "projects/dashboards-ng",
    "projects/dashboards-demo"
  ],
  "dependencies": {
    "@angular-architects/module-federation": "20.0.0",
    "@angular/animations": "20.0.6",
    "@angular/cdk": "20.0.6",
    "@angular/common": "20.0.6",
    "@angular/core": "20.0.6",
    "@angular/elements": "20.0.6",
    "@angular/forms": "20.0.6",
    "@angular/platform-browser": "20.0.6",
    "@angular/router": "20.0.6",
    "@ngx-formly/bootstrap": "^6.2.2",
    "@ngx-formly/core": "^6.2.2",
    "@ngx-translate/core": "16.0.4",
    "@siemens/ngx-datatable": "^24.0.0",
    "echarts": "5.6.0",
    "flag-icons": "7.5.0",
    "google-libphonenumber": "3.2.42",
    "gridstack": "11.5.1",
    "ngx-image-cropper": "^9.0.0",
    "rxjs": "7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular-builders/custom-esbuild": "20.0.0",
    "@angular-devkit/build-angular": "20.0.6",
    "@angular/build": "20.0.6",
    "@angular/cli": "20.0.6",
    "@angular/compiler": "20.0.6",
    "@angular/compiler-cli": "20.0.6",
    "@angular/language-service": "20.0.6",
    "@angular/localize": "20.0.6",
    "@axe-core/playwright": "4.10.2",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@eslint/js": "9.32.0",
    "@playwright/test": "1.54.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@siemens/commitlint-config": "2.3.1",
    "@siemens/eslint-config-angular": "2.3.1",
    "@siemens/eslint-plugin-defaultvalue": "2.3.1",
    "@siemens/prettier-config": "2.3.1",
    "@siemens/stylelint-config-scss": "2.3.1",
    "@types/google-libphonenumber": "^7.4.23",
    "@types/jasmine": "5.1.8",
    "@types/node": "~22.17.0",
    "angular-eslint": "20.1.1",
    "axe-html-reporter": "2.2.11",
    "concat": "1.0.3",
    "cpy-cli": "5.0.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.28.0",
    "eslint-plugin-headers": "1.3.3",
    "eslint-plugin-perfectionist": "4.15.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-tsdoc": "0.4.0",
    "http-server": "14.1.1",
    "husky": "9.1.7",
    "jasmine-core": "5.9.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-firefox-launcher": "2.1.3",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "karma-jasmine-seed-reporter": "0.2.0",
    "karma-junit-reporter": "2.0.1",
    "karma-spec-reporter": "0.0.36",
    "ng-packagr": "20.1.0",
    "postcss": "8.5.6",
    "postcss-scss": "4.0.9",
    "prettier": "3.6.2",
    "semantic-release": "24.2.6",
    "stylelint": "16.23.0",
    "stylelint-scss": "6.12.1",
    "stylelint-use-logical-spec": "5.0.1",
    "typescript": "5.8.3",
    "typescript-eslint": "8.39.0"
  },
  "optionalDependencies": {
    "@simpl/brand": "^0.3.5",
    "@simpl/docs-composer": "^11.12.0",
    "@simpl/element-icons": "^1.28.0",
    "cross-env": "^10.0.0"
  }
}
